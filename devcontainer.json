{
  "name": "wheele",
  "dockerComposeFile": "docker-compose.yml",
  "service": "robot1",
  "workspaceFolder": "/home/ubuntu/ros2_ws",
  "workspaceMount": "source=${localWorkspaceFolder}/src,target=/home/ubuntu/ros2_ws/src,type=bind",
//  "workspaceMount": "source=${localWorkspaceFolder}/launch,target=/home/ubuntu/ros2_ws/launch,type=bind",
//  "workspaceMount": [
//    "source=${localWorkspaceFolder}/src,target=/home/ubuntu/ros2_ws/src,type=bind",
//    "source=${localWorkspaceFolder}/launch,target=/home/ubuntu/ros2_ws/launch,type=bind"
//  ],
  "remoteUser": "ubuntu",

  "customizations": {
    "jetbrains": {
      "backend": "CLion",
      "plugins": [
        "com.intellij.bash",
        "Docker",
        "org.jetbrains.plugins.terminal",
        "com.jetbrains.plugins.cmake",
        "Python"
      ]
    }
  },

  "forwardPorts": [6080, 5901],

  "portsAttributes": {
    "6080": { "label": "noVNC Web", "protocol": "http" }
  },

  "shutdownAction": "stopCompose",

  "overrideCommand": false,

  "runServices": ["robot1"],

//  "workspaceMount": "source=${localWorkspaceFolder}/ros2_ws/src,target=/home/ubuntu/ros2_ws/src,type=bind,consistency=cached"
}
